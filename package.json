{
  "name": "treetracker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test-unit; npm run test-integration;npm run test-repository",
    "test-unit": "NODE_ENV=test mocha --exit ./server/models/**/*.spec.js ./server/routes/**/*.spec.js",
    "test-repository": "NODE_ENV=test mocha --exit --require co-mocha  ./server/repositories/**/*.spec.js",
    "serverTest": "cd ./server && DEBUG=loopback:*,express:* nodemon server.js",
    "server": "cd ./server && nodemon server.js",
    "test-seedDB": "NODE_ENV=test mocha --timeout 10000 --require co-mocha './**/*.spec.js'",
    "test-integration": "NODE_ENV=test mocha --exit --timeout 20000 --require co-mocha './__tests__/supertest.js'",
    "test-watch": "NODE_ENV=test NODE_LOG_LEVEL=info mocha --timeout 10000 --require co-mocha -w -b --ignore './server/repositories/**/*.spec.js'  './server/setup.js' './server/**/*.spec.js' './__tests__/supertest.js'",
    "test-watch-debug": "NODE_ENV=test NODE_LOG_LEVEL=debug mocha --timeout 10000 --require co-mocha -w -b --ignore './server/repositories/**/*.spec.js'  './server/setup.js' './server/**/*.spec.js' './__tests__/supertest.js'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "expect": "^26.4.2",
    "express": "^4.16.2",
    "express-async-handler": "^1.1.4",
    "express-bearer-token": "^2.1.1",
    "express-validator": "^6.4.0",
    "joi": "^17.2.1",
    "jsonwebtoken": "8.1.0",
    "knex": "^0.21.5",
    "loglevel": "^1.6.8",
    "mocha": "^8.1.3",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.4",
    "pg": "^7.4.0",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "chai-http": "^4.3.0",
    "co-mocha": "1.1.2",
    "database-cleaner": "^1.3.0",
    "db-migrate": "^0.10.7",
    "db-migrate-pg": "^0.4.0",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "expect-runtime": "^0.7.0",
    "mock-knex": "^0.4.9",
    "prettier": "^2.1.2",
    "sinon": "^9.0.3",
    "sinon-chai": "^2.14.0",
    "standard": "^14.3.4",
    "supertest": "^4.0.2"
  }
}
